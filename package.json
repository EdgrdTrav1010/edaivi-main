{
  "name": "edaivi-studio",
  "version": "1.0.0",
  "description": "AI-powered multimedia platform by EdAiVi Studio",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo 'Backend build complete'",
    "dev:frontend": "cd frontend && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "setup:db": "node scripts/setup-db.js",
    "setup:storage": "node scripts/setup-storage.js"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "aws-sdk": "^2.1450.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^16.8.0",
    "graphql-http": "^1.21.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.5",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^4.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.8",
    "sequelize": "^6.32.1",
